{
  "name": "IsomorphicBoilerplate",
  "version": "0.0.1",
  "description": "Isomorphic React Boilerplate",
  "engines": {
    "node": ">=6.10.0"
  },
  "main": "index.js",
  "scripts": {
    "server": "npm run build; babel-node --presets react,es2015 src/server.js",
    "build": "webpack --config ./webpack.config.js",
    "start": "WEBPACK_DEV=true webpack-dev-server --config ./webpack.config.js --content-base ./src/views/ --inline --progress",
    "lint": "standard",
    "fixlinting": "standard --fix",
    "ci": "TEST_REPORT_DIRECTORY=${1:-unit}; istanbul cover _mocha --  test/ -R xunit > $CIRCLE_TEST_REPORTS/$TEST_REPORT_DIRECTORY/results.xml",
    "prerelease": "node_modules/@holidayextras/deployment-helpers/nodeApps/preRelease.sh",
    "pretest": "npm run prerelease",
    "test": "npm run lint && npm run test:coverage",
    "test:watch": "mocha --watch",
    "test:coverage": "istanbul cover _mocha",
    "posttest": "istanbul check-coverage",
    "postinstall": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@holidayextras/deployment-helpers": "*",
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.2",
    "istanbul": "v1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "standard": "^8.6.0",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "dependencies": {
    "consolidate": "^0.14.5",
    "express": "^4.15.2",
    "mustache": "^2.3.0",
    "react-router-dom": "^4.0.0"
  }
}
